{
    "name": "maginium/installer",
    "description": "Maginium application installer.",
    "keywords": [
        "maginium"
    ],
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@maginium.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/filesystem": "^11.43",
        "illuminate/support": "^11.43",
        "laravel/prompts": "^0.3.0",
        "povils/figlet": "^0.1.0",
        "symfony/console": "^7.0",
        "symfony/polyfill-mbstring": "^1.31",
        "symfony/process": "^7.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.4",
        "symfony/var-dumper": "^7.2"
    },
    "scripts": {
        "format": "./vendor/bin/pint",
        "format:copyright": "php ./scripts/CopyrightManager.php",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "bin": [
        "bin/maginium"
    ],
    "autoload": {
        "files": [
            "functions/dump.php",
            "functions/base.php",
            "functions/config-registration.php",
            "functions/command-registration.php"
        ],
        "psr-4": {
            "Maginium\\Installer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maginium\\Installer\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}